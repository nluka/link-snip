{
  "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    // "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "off"
  },
  "globals": {
    "axios": true
  }
}
